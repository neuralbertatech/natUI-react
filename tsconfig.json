{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "declaration": true,
    "outDir": "./lib",
    "sourceMap": true,
    "baseUrl": ".",
    "allowJs": false
  },
  "include": ["src"]
}
