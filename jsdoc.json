{
  "opts": {
    "destination": "docs/",
    "encoding": "utf-8",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/better-docs",
    "verbose": true
  },
  "plugins": [
    "node_modules/better-docs/component",
    "node_modules/better-docs/typescript"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(node_modules/|docs|index.ts)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnkownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "better-docs": {
      "css": "styles/style.css",
      "logo": "images/natLogo.svg",
      "name": "",
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/neuralbertatech/natUI-react"
        },
        {
          "label": "npm",
          "href": "https://www.npmjs.com/package/@neuralbertatech/ui-react"
        }
      ],
      "title": "natUI-react docs |"
    },
    "search": true,
    "default": {
      "staticFiles": {
        "include": [
          "./better-docs"
        ]
      }
    }
  }
}
